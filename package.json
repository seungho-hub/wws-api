{
  "name": "wws-api-server",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/seungho-hub/wws-api-server.git",
  "author": "seungho-hub <63999413+kangseungho-hub@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
    "build": "tsc",
    "db:migrate:prod": "dotenv -e ./.env -- prisma migrate deploy",
    "db:migrate:dev": "dotenv -e ./.env -- prisma migrate dev",
    "db:migrate:test": "dotenv -e ./.env.test -- prisma migrate dev",
    "db:push:dev": "dotenv -e ./.env -- prisma db push",
    "db:push:test": "dotenv -e ./.env.test -- prisma db push",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration --runInBand"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "@types/cookie": "^0.6.0",
    "connect-redis": "^7.1.1",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jest-mock-extended": "^3.0.7",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "redis": "^4.7.0",
    "supertest": "^7.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.15",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.1",
    "@types/redis": "^4.0.11",
    "@types/serve-favicon": "^2.5.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.43.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
